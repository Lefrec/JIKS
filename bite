for ( let i=0; i<squareNumber; i++) {
    document.getElementById("body").innerHTML += 
    `<div style="
    position: absolute;
    height: 10px;
    width: 10px;
    top: 0;
    left: 0;
    background-color: white;" id="square${squareNumber}">
    </div>`;
    testRay.push(document.getElementById(`square${squareNumber}`));
    squareNumber++;
}